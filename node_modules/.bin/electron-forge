#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/admini/Desktop/my-electron-app/node_modules/.store/@electron-forge+cli@7.3.1/node_modules/@electron-forge"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/admini/Desktop/my-electron-app/node_modules/.store/@electron-forge+cli@7.3.1/node_modules/@electron-forge"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/@electron-forge+cli@7.3.1/node_modules/@electron-forge/cli/dist/electron-forge.js" "$@"
else
  exec node  "$basedir/../.store/@electron-forge+cli@7.3.1/node_modules/@electron-forge/cli/dist/electron-forge.js" "$@"
fi
